define(action,%arg1) // change action here

[ENABLE]
alloc(MobControl,128)

MobControl:
mov [edi+000004D0],action
mov eax,[edi+000004D0]
jmp 026AC967+6

026AC967:
jmp MobControl
db 90

[DISABLE]
026AC967: // CVecCtrlMob::WorkUpdateActive: E9 ? ? ? ? 8B ? ? ? 00 00 83 ? ? 0F ? ? ? ? ? FF [Address below]
mov eax,[edi+000004D0]
 
dealloc(MobControl)
