[ENABLE]
alloc(Hook,128)
alloc(Skill_ID,4)
registersymbol(Skill_ID)

Hook:
mov [Skill_ID],ecx
cmp byte ptr [ebp+28],00
jmp 021EC9D4+7

021EC9D4:
jmp Hook
db 90 90

[DISABLE]
021EC9D4: // 89 ? ? 80 ? ? ? 89 ? ? ? ? ? 74
mov [ebx+38],ecx
cmp byte ptr [ebp+28],00

dealloc(Hook)
dealloc(Skill_ID)
unregistersymbol(Skill_ID)